header{
	font-size: 0.18rem;
	line-height: 0.43rem;
	color: #fff;
	text-align: center;
}

header>a{
	display: block;
    position: absolute;
    width: 0.14rem;
    height: 0.44rem;
    background: url(../img/return.png) no-repeat left center;
    background-size: 0.1rem;
    margin-left: 0.1rem;
}

section{
	margin-top: 0.43rem;
	width: 100%;
	border-top: 1px solid #f8f8f8;
	box-sizing: border-box;
}

section>.list{
	width: 100%;
    height: 0.98rem;
    padding: 0.1rem 0;
    border-bottom: 1px solid #dadada;
    background: #fff;
}

section>.list>a{
	display: block;
    width: 95%;
    height: 0.98rem;
    margin: 0 auto;
}

section>.list>a>.list_left{
	width: 20%;
	height: 0.98rem;
    float: left;
    text-align: center;
}

section>.list>a>.list_left>img{
	display: block;
	height: 0.5rem;
	margin: 0.24rem auto;
}

section>.list>a>.list_mid{
	width: 55%;
	height: 0.98rem;
    float: left;
}

section>.list>a>.list_mid>p:first-child{
	height: 0.36rem;
    font-size: 0.14rem;
    color: #333;
    padding-top: 0.05rem;
}

section>.list>a>.list_mid>p:nth-child(2){
	height: 0.27rem;
	font-size: 0.12rem;
	line-height: 0.27rem;
	color: #333;
}

.blue{
	color: #00A0E9;
}

section>.list>a>.list_mid>.barbox{
	width: 88%;
	height: 0.05rem;
	background: #f0f0f0;
	border-radius: 0.05rem;
	margin-bottom: 0.1rem;
}

section>.list>a>.list_mid>.barbox>.bar{
	width: 78%;
	height: 100%;
	border-radius: 0.05rem;
	background: #f2473d;
}

section>.list>a>.list_mid>span{
	font-size: 0.09rem;
	display: block;
	float: left;
}

section>.list>a>button{
	width: 0.59rem;
	height: 0.23rem;
	background: #F2473D;
	border: none;
	border-radius: 0.23rem;
	color: #fff;
	font-size: 0.12rem;
	float: right;
	margin-top: 0.375rem;
}
