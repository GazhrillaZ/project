/*-----------------------------------------------分类classify-------------------------------------------------*/

header{
	font-size: 0.18rem;
	line-height: 0.43rem;
	color: #fff;
	text-align: center;
}

header>a{
	display: block;
    position: absolute;
    width: 0.14rem;
    height: 0.44rem;
    background: url(../img/return.png) no-repeat left center;
    background-size: 0.1rem;
    margin-left: 0.1rem;
}

section{
	margin-top: 0.43rem;
	width: 100%;
	border-top: 1px solid #f8f8f8;
	box-sizing: border-box;
}

section>a{
	width: 96.875%;
	height: 0.52rem;
	border-bottom: 1px solid #e2e2e2;
	float: right;
	font-size: 0.15rem;
	color: #333;
	line-height: 0.52rem;
}

section>a>img{
	width: 0.21rem;
	margin-left: 0.15rem;
	margin-right: 0.17rem;
	vertical-align: middle;
}

/*-------------------------------------------手机平板iPhone---------------------------------------------*/

section>.iPhone_list{
	width: 100%;
    height: 0.98rem;
    padding: 0.1rem 0;
    border-bottom: 1px solid #dadada;
    background: #fff;
}

section>.iPhone_list>a{
	display: block;
    width: 95%;
    height: 0.98rem;
    margin: 0 auto;
}

section>.iPhone_list>a>.list_left{
	width: 20%;
	height: 0.98rem;
    float: left;
    text-align: center;
}

section>.iPhone_list>a>.list_left>img{
	display: block;
	height: 0.5rem;
	margin: 0.24rem auto;
}

section>.iPhone_list>a>.list_mid{
	width: 55%;
	height: 0.98rem;
    float: left;
}

section>.iPhone_list>a>.list_mid>p:first-child{
	height: 0.36rem;
    font-size: 0.14rem;
    color: #333;
    padding-top: 0.05rem;
}

section>.iPhone_list>a>.list_mid>p:nth-child(2){
	height: 0.27rem;
	font-size: 0.12rem;
	line-height: 0.27rem;
	color: #333;
}

.blue{
	color: #00A0E9;
}

section>.iPhone_list>a>.list_mid>.barbox{
	width: 88%;
	height: 0.05rem;
	background: #f0f0f0;
	border-radius: 0.05rem;
	margin-bottom: 0.1rem;
}

section>.iPhone_list>a>.list_mid>.barbox>.bar{
	width: 78%;
	height: 100%;
	border-radius: 0.05rem;
	background: #f2473d;
}

section>.iPhone_list>a>.list_mid>span{
	font-size: 0.09rem;
	display: block;
	float: left;
}

section>.iPhone_list>a>button{
	width: 0.59rem;
	height: 0.23rem;
	background: #F2473D;
	border: none;
	border-radius: 0.23rem;
	color: #fff;
	font-size: 0.12rem;
	float: right;
	margin-top: 0.375rem;
}


/*-----------------------------------购物车cart-----------------------------------*/

section>.empty{
	width: 1.26rem;
	height: 1.87rem;
	margin: auto;
	margin-top: 1.05rem;
}

section>.empty>img{
	width: 0.5rem;
	margin: auto;
	display: block;
}

section>.empty>span{
	display: block;	
	width: 100%;
	margin-top: 0.12rem;
	font-size: 0.16rem;
    line-height: 0.24rem;
    color: #666666;
    text-align: center;
}

section>.empty>a{
    width: 1.26rem;
    height: 0.35rem;
    background: #F2473D;
    margin: auto;
    margin-top: 0.3rem;
    display: block;
    color: #fff;
    line-height: 0.35rem;
    text-align: center;
    font-size: 0.16rem;
    border-radius: 0.05rem;	
}

section>.cartable{
	width: 100%;
}

section>.cartable>.itembox{
	width: 100%;
	height: 0.78rem;
	background: #fff;
    border-bottom: 1px solid #e5e5e5;
    border-top: 1px solid #e5e5e5;
    margin-bottom: 0.06rem;
    position: relative;
    padding: 0.19rem 0;
}

section>.cartable>.itembox>.item{
	width: 93.75%;
	height: 100%;
	margin: auto;
}

section>.cartable>.itembox>.item>.check{
	display: block;
    width: 0.17rem;
    height: 0.17rem;
    cursor: pointer;
    margin-top: 0.3rem;
    margin-right: 0.1rem;
    float: left;
    background: url(../img/calselno.png) no-repeat #fff;
    background-size: 17px 17px;
}

section>.cartable>.itembox>.item>.img{
	width: 0.74rem;
    height: 0.74rem;
    overflow: hidden;
    border: 1px solid #e5e5e5;
    margin-right: 0.10rem;
    display: block;
    float: left;
}

section>.cartable>.itembox>.item>.img>img{
	width: 0.74rem;
    height: 0.74rem;
    display: block;
}

section>.cartable>.itembox>.item>.content{
	width: 1.87rem;
	height: 0.78rem;
	float: left;
}

section>.cartable>.itembox>.item>.content>.title{
	color: #333333;
    font-size: 0.14rem;
    line-height: 0.14rem;
    font-weight: normal;
    height: 0.14rem;
    overflow: hidden;
    display: block;
}

section>.cartable>.itembox>.item>.content>.num{
	height: 0.25rem;
    overflow: hidden;
    margin: 0.1rem 0;
}

section>.cartable>.itembox>.item>.content>.num>input{
	width: 0.25rem;
	height: 0.25rem;
	float: left;
    color: #9e9e9e;
    border: 1px solid #ddd;
    box-sizing: border-box;
    background: #fff;
    font-size: 0.12rem;
    text-align: center;
    outline: none;
}

section>.cartable>.itembox>.item>.content>.num>.btn{
	float: right;
    width: 0.64rem;
    background: #F2473D;
    height: 0.25rem;
    color: #fff;
    font-size: 0.12rem;
    text-align: center;
    line-height: 0.24rem;
    border-radius: 0.13rem;
    cursor: pointer;
}

section>.cartable>.itembox>.item>.content>p{
	font-size: 0.09rem;
	line-height: 0.18rem;
	color: #999999;
	display: block;
	float: left;
}

section>.cartable>.itembox>.item>.content>.first{
	width: 0.88rem;
}

section>.cartable>.itembox>.item>.content>p>span{
	color: #00a0e9;
}

section>.cartable>.total{
	width: 100%;
	height: 0.4rem;
	background: #fff;
	position: fixed;
	left: 0;
	bottom: 0.5rem;
}

section>.cartable>.total>.check{
	width: 0.17rem;
	height: 0.17rem;
	margin: 0.11rem 0.1rem;
	display: block;
	float: left;
}

section>.cartable>.total>.choose{
	font-size: 0.14rem;
	line-height: 0.4rem;
	float: left;
}

section>.cartable>.total>p{
	font-size: 0.12rem;
	line-height: 0.4rem;
	float: left;
	margin-left: 0.13rem;
}

section>.cartable>.total>p>span{
	color: #F2473D;
}

section>.cartable>.total>div{
	font-size: 0.12rem;
	line-height: 0.4rem;
	color: #fff;
	text-align: center;
	float: right;
	height: 100%;
}

section>.cartable>.total>.account{
	width: 0.83rem;
	background: #f2473d;
}

section>.cartable>.total>.del{
	width: 0.78rem;
	background: #ff9402;
}

section>.cartable>.nothing{
	position: fixed;
    left: 50%;
    bottom: 50%;
    color: #fff;
    border-radius: 0.05rem;
    width: 2.56rem;
    height: 0.46rem;
    margin-left: -1.28rem;
    line-height: 0.45rem;
    text-align: center;
    background: rgba(0, 0, 0, 0.6);
    box-shadow: 0 2px 3px #9b9b9b;
    font-size: 0.14rem;
    z-index: 10000;
}

.active{
	border-color: red !important;
}


/*----------------------------发现discover-----------------------------------------------*/


section>.discover_list{
	width: 100%;
    height: 0.6rem;
    background: #fff;
    border-bottom: 1px solid #dadada;
}

section>.discover_list>a{
	display: block;
    width: 90%;
    height: 0.6rem;
    margin: 0 auto;
}

section>.discover_list>a>.img{
	width: 19%;
    height: 0.6rem;
    float: left;
}

section>.discover_list>a>.img>img{
	width: 0.4rem;
    margin-top: 0.1rem;
    display: block;
}

section>.discover_list>a>.text{
	width: 76%;
    height: 0.6rem;
    float: left;
}

section>.discover_list>a>.text>.title{
	font-size: 0.15rem;
	padding-top: 0.1rem;
    color: #333;
}

section>.discover_list>a>.text>.content{
	font-size: 0.1rem;
	color: #666;
}

section>.discover_list>a>img{
	width: 0.1rem;
    margin-top: 0.25rem;
    float: right;
}


/*----------------------------------------云购记录record---------------------------------*/

.redpack_topmenu{
	width: 100%;
	height: 0.4rem;
	background: #fff;
	margin-top: 0.43rem;
	border-bottom: 1px solid #e2e2e2;
}

.redpack_topmenu>li{
	width: 0.5rem;
	height: 0.4rem;
	line-height: 0.4rem;
	border-bottom: 0.02rem solid transparent;
	font-size: 0.15rem;
	color: #3e3a39;
	float: left;
	margin-left:0.283rem ;
	margin-right: 0.283rem;
	box-sizing: border-box;
	text-align: center;
}

.redpack_topmenu>.redpackBtn_active{
	color: #f2473d;
	border-color: #f2473d;
}

.redpack_detail{
	width: 100%;
}

.redpack_detail>ul{
	width: 100%;
	height: 100%;
	display: none;
}

.redpack_detail>.detail_active{
	display: block;
}

.redpack_detail>ul>li{
	width: 100%;
	height: 1.47rem;
	border-bottom: 1px solid #e2e2e2;
	background: #fff;
}

.redpack_detail>ul>li>.item{
	width: 3rem;
	height: 1.47rem;
	margin: auto;
}

.redpack_detail>ul>li>.item>img{
	width: 0.5rem;
	height: 0.5rem;
	float: left;
	margin-top: 0.485rem;
}

.redpack_detail>ul>li>.item>.text{
	width: 1.75rem;
	height: 1.22rem;
	margin-top: 0.25rem;
	margin-left: 0.12rem;
	float: left;
}

.redpack_detail>ul>li>.item>.text>p{
	font-size: 0.11rem;
}

.blue{
	color: #00a0e9;
}

.red{
	color: #f3534a;
}

.redpack_detail>ul>li>.item>.btns{
	width: 0.53rem;
	height: 1.47rem;
	float: right;
}

.redpack_detail>ul>li>.item>.btns>a{
	width: 0.53rem;
	height: 0.22rem;
	font-size: 0.12rem;
	display: block;
	text-align: center;
	border-radius: 0.02rem;
	box-sizing: border-box;
	line-height: 0.22rem;
}

.redpack_detail>ul>li>.item>.btns>a:first-child{
	color: #fff;
	background: #f2473d;
	margin-top: 0.275rem;
}

.redpack_detail>ul>li>.item>.btns>a:last-child{
	color: #f2473d;
	background: #fff;
	border:1px solid #f2473d;
	margin-top: 0.39rem;
}


/*---------------------------------------------注册signup-------------------------------------------*/

section>.signup_form{
	width: 2.9rem;
	height: auto;
	margin: 0.28rem auto 0;
}

section>.signup_form>div{
	width: 100%;
	height: 0.4rem;
	background: #fff;
	border-bottom: 1px solid #e2e2e2;
	box-sizing: border-box;
	font-size: 0.15rem;
	line-height: 0.4rem;
	color: #333;
	padding-left: 0.12rem;
}

section>.signup_form>div>input{
	border: none;
	height: 0.2rem;
	outline: none;
}

section>.signup_form>input{
	width: 100%;
	height: 0.4rem;
	display: block;
	margin-top: 0.15rem;
	background: #f2473d;
	border: none;
	color: #FFFFFF;
	border-radius: 0.075rem;
	font-size: 0.15rem;
}

section>.signup_form>div>#security{
	width: 0.92rem;
	height: 0.3rem;
	background: #f2473d;
	float: right;
	margin-top: 0.05rem;
	margin-right: 0.05rem;
	color: #fff;
	font-size: 0.15rem;
	text-align: center;
	border-radius: 0.05rem;
}

section>.signup_form>div>a{
	float: right;
	margin-top: 0.05rem;
	margin-right: 0.05rem;
}

section>.signup_form>div>a>img{
	width: 0.4rem;
}

section>.signup_form>p{
	width: 100%;
	color: #999999;
	font-size: 0.1rem;
	display: block;
	margin-top: 0.1rem;
	text-align: center;
}

section>.signup_form>p>a{
	color: #00a0e9;
}

