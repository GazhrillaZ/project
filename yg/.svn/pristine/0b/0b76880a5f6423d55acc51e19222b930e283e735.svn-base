
.index_sign{
	width: 100%;
	min-height: 100%;
	background: rgba(0,0,0,0.5);
	position: absolute;
	top: 0;
	left: 0;
	z-index: 222222;
	display: none;
}

.sign_light{
	width: 2.17rem;
	height: 2.17rem;
	margin: auto;
	margin-top: 1.56rem;
	background: url(../img/light.png);
	background-size: cover;
	text-align: center;
}

.sign_light>img{
	width: 1.95rem;
	display: block;
	margin: auto;
	margin-bottom: 0.14rem;
}


.index_banner{
	width: 100%;
	height: 1.2rem;
	margin-top: 0.43rem;
	position: relative;
}
.index_banner>ul{
	width: 100%;
	height: 100%;
	position: relative;
}
.index_banner>ul>li{
	width: 100%;
	height: 100%;
	position: absolute;
	top: 0;
	left: 0;
}



body{
	background: #FFFFFF;
}
.index_banner>ul>li>img{
	width: 100%;
	display: block;
}

.index_banner>ol{
	width: 0.5rem;
	height: 0.05rem;
	position: absolute;
	left: 43.5%;
	top:1.07rem;
	
}

.index_banner>ol>li{
	width: 0.05rem;
	height: 0.05rem;
	border-radius: 0.05rem;
	background: #e2e2e2;
	margin-right: 0.075rem;
	float: left;
	box-sizing: border-box;
}

.index_banner>ol>.bannerBtn_active{
	background:#fff ;
	border:1px solid red ;
}

.index_nav{
	width: 100%;
	height: 0.8rem;
	border-bottom: 1px solid #e2e2e2;
	border-top: 1px solid #e2e2e2;
}

.index_clearfix{
	width: 100%;
	height: 100%;
}

.index_clearfix>li{
	width: 20%;
	height: 100%;
	float: left;
	
}
.index_clearfix>li>a{
	width: 100%;
	height: 100%;
	display: block;
	box-sizing: border-box;
	border-top:1px solid #fff;
}

.index_clearfix>li>a>img{
	display: block;
	width: 0.35rem;
	margin: auto;
	margin-top: 0.15rem;
}

.index_clearfix>li>a>p{
	font-size: 0.1rem;
	text-align: center;
	margin-top: 0.075rem;
}

.index_noticeBar{
	width: 100%;
	height: 0.36rem;
	font-size: 0.11rem;
	line-height: 0.36rem;
}

.index_noticeBar>img{
	width: 0.28rem;
	vertical-align: middle;
	margin-left: 0.14rem;
	margin-right: 0.1rem;
}

.index_noticeBar>span{
	line-height: 0.36rem;
}

.index_greyBar{
	width: 100%;
	height: 0.09rem;
	background: #f0f0f0;
}

.index_goodList{
	width: 100%;
	height: auto;
}

.goods_topmenu{
	width: 100%;
	height: 0.41rem;
	border-bottom: 1px solid #F0F0F0;
}

.goods_topmenu>ul{
	width: 2.95rem;
	height: 0.41rem;
	margin: auto;
}

.goods_topmenu>ul>li{
	width: 25%;
	height: 0.41rem;
	border-bottom: 2px solid transparent;
	box-sizing: border-box;
	float: left;
	font-size: 0.15rem;
	line-height: 0.41rem;
	text-align: center;
}

.goods_topmenu>ul>.menu_active{
	color:#f2473d;
	border-color:#f2473d;
}

.goods_something,.goods_something>ul{
	width: 100%;
	height: auto;
}

.goods_something>ul>li{
	width: 50%;
	height: 2.4rem;
	/*padding-top: 0.1rem;*/
	border-left: 1px solid #F0F0F0;
	float: left;
	box-sizing: border-box;
	margin-top: 0.1rem;
}

.goods_something>ul>li>img{
	width: 1.4rem;
	margin: auto;
	margin-bottom: 0.16rem;
	display: block;
}

.goods_something>ul>li>.goods_name{
	font-size: 0.15rem;
	color:#373737;
	text-indent: 0.1rem;
}

.goods_progress{
	width: 1.4rem;
	height: 0.3rem;
	margin: auto;
	margin-top: 0.12rem;
}

.progress_left{
	width: 0.75rem;
	height: 0.3rem;
	float: left;
}

.progress_left>p{
	font-size: 0.14rem;
	color:#373737;
}

.progress_bar{
	width: 0.75rem;
	height: 0.05rem;
	border-radius: 0.1rem;
	background: #f0f0f0;
	margin-top: 0.06rem;
}

.progress_bar>.progress_insideBar{
	width: 0.25rem;
	height: 100%;
	border-radius: 0.1rem;
	background: #f2473d;
}

.progress_right{
	width: 0.3rem;
	height: 0.3rem;
	float: right;
}
.progress_right>img{
	width: 0.3rem;
	display: block;
}
