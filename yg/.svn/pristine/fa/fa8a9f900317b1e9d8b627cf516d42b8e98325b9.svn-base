.top_fixed>.personal_shao{
	width: 0.185rem;
	height: 0.21rem;
	display: block;
	position: absolute;
	top:0.115rem;
	right: 0.22rem;
}

.personal_name{
	width: 100%;
	height: 0.8rem;
	background: #fff;
	margin-top: 0.59rem;
	margin-bottom: 0.15rem;
	border-top: 1px solid transparent;
	box-sizing: border-box;
}

.personal_name>.personal_nameIn{
	width: 3.01rem;
	height: 0.54rem;
	margin: auto;
	margin-top: 0.13rem;
}

.personal_name>.personal_nameIn>.personal_left{
	width: 0.54rem;
	height: 100%;
	float: left;
}

.personal_name>.personal_nameIn>.personal_left>img{
	width: 0.54rem;
	display: block;
}

.personal_name>.personal_nameIn>.personal_middle{
	width: 1.95rem;
	height: 100%;
	float: left;
	margin-left: 0.1rem;
}

.personal_name>.personal_nameIn>.personal_middle>p:first-child{
	font-size: 0.14rem;
	color: #333333;
	margin-bottom: 0.09rem;
}

.personal_name>.personal_nameIn>.personal_middle>p:last-child{
	font-size: 0.12rem;
	color: #666666;
}

.personal_name>.personal_nameIn>.personal_right{
	width: 0.38rem;
	height: 100%;
	float:left;
}

.personal_name>.personal_nameIn>.personal_right>img:first-child{
	width: 0.18rem;
	margin-top: 0.18rem;
	display: block;
	float: left;
}

.right_arrow{
	width: 0.06rem;
	margin-top:0.21rem;
	display: block;
	float: right;
}

.personal_menuList,.personal_menuList>ul{
	width: 100%;
	height: auto;
	background: #fff;
}

.personal_menuList>ul>li{
	width: 100%;
	height: 0.32rem;
	padding-top: 0.12rem;
	border-bottom: 1px solid #F0F0F0;
}

.personal_menuList>ul>li>.personal_menu{
	width: 3.01rem;
	height: 0.21rem;
	margin: auto;
	display: block;
}

.personal_menu>img:first-child{
	width: 0.21rem;
	height: 0.21rem;
	display: block;
	float: left;
}

.personal_menu>span{
	width: 0.6rem;
	height: 0.21rem;
	line-height: 0.21rem;
	font-size: 0.15rem;
	color:#333333;
	display: block;
	float: left;
	margin-left: 0.15rem;
}

.personal_menu>p{
	width: 1.15rem;
	height: 0.21rem;
	line-height: 0.21rem;
	font-size: 0.11rem;
	float: left;
	margin-left: 0.75rem;
}

.personal_menu>p>span{
	color:red;
}

.right_arrow2{
	width: 0.06rem;
	margin-top: 0.045rem;
	display: block;
	float: right;
}

.personal_menuList>ul>.seperate_bar{
	width: 100%;
	height: 0.1rem;
	background: #F0F0F0;
}
