/*cashdetail*/

.cashin{
	width: 100%;
	height: 0.85rem;
	padding-top: 0.2rem;
	background: #fff;
	margin-top: 0.43rem;
}

.cashin>ul{
	width: 100%;
	height: 0.5rem;
}

.cashin>ul>li{
	width: 33%;
	height: 100%;
	font-size: 0.12rem;
	color: #666666;
	text-align: center;
	float: left;
}

.cashin>ul>li>p{
	margin-bottom: 0.05rem;
}

.cashin>.application{
	width: 100%;
	height: 0.25rem;
	font-size: 0.12rem;
	line-height: 0.25rem;
}

.application>a:first-child{
	width: 0.7rem;
	height: 100%;
	background: #f2473d;
	color: #FFF;
	display: block;
	float: left;
	border-radius: 0.25rem;
	line-height: 0.25rem;
	text-align: center;
	margin-left: 0.2rem;
	margin-right: 0.1rem;
}

.application>a:last-child{
	margin-left: 0.3rem;
	color: #00a0e9;
}

.cash_return{
	width: 100%;
	height: auto;
	margin-top: 0.15rem;
}

.cash_return>ul:first-child{
	width: 100%;
	height: 0.4rem;
	line-height: 0.4rem;
	background: #fff;
}

.cash_return>ul:first-child>li{
	width: 33%;
	height: 100%;
	display: block;
	float: left;
	text-align: center;
	color: #666666;
	font-weight: bold;
	box-sizing: border-box;
	font-size: 0.12rem;
}

.cash_return>ul:first-child>.cashreturn_active{
	color: #f2473d;
	border-bottom: 2px solid #f2473d;
}

.cash_return>ul:last-child{
	width: 100%;
	height: auto;
	background: #FFFFFF;
	margin-top: 0.05rem;
}

.cash_return>ul:last-child>li{
	width: 100%;
	height: 0.35rem;
	font-size: 0.1rem;
	line-height: 0.3rem;
	border-bottom: 1px solid #F0F0F0;
}

.cash_return>ul:last-child>li>span{
	width: 33%;
	height: 100%;
	display: block;
	float: left;
	text-align: center;
	color: #666666;
}

.cash_return>ul:last-child>li>span:last-child{
	color: #F2473D;
}

/*cashlist*/

.cashlist{
	width: 100%;
	height: auto;
	margin-top: 0.43rem;
	background: #fff;
}
.cashlist>li{
	width: 100%;
	height: 0.3rem;
	line-height: 0.3rem;
	color: #666666;
	font-size: 0.12rem;
	border-bottom: 1px solid #F0F0F0;
}

.cashlist>li:first-child{
