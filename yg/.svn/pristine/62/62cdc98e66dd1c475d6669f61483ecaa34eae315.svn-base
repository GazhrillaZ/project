/*charge*/

.chargecash{
	width: 100%;
	height: auto;
	margin-top: 0.43rem;
	font-size: 0.12rem;
}

.chargecash>p{
	text-indent: 0.1rem;
	line-height: 0.44rem;
	background: #fff;
}

.chargecash>ul{
	width: 100%;
	height:1.3rem;
	border-top: 1px solid #F0F0F0;
	margin-bottom: 0.15rem;
	background: #fff;
}

.chargecash>ul>li{
	width: 0.9rem;
	height: 0.4rem;
	text-align: center;
	line-height: 0.4rem;
	float: left;
	margin-left: 0.1rem;
	border: 1px solid #F0F0F0;
	border-radius: 0.1rem;
	margin-top: 0.15rem;
	position: relative;
}

.chargecash>ul>li>.trangle{
	width: 0;
	height: 0;
	border-bottom: 0.125rem solid #F0F0F0;
	border-right: 0.125rem solid #F0F0F0;
	border-top: 0.125rem solid transparent;
	border-left: 0.125rem solid transparent;
	border-radius: 0 0 0.1rem 0;
	position: absolute;
	bottom: 0;
	right: 0;
}

.chargecash>ul>li:first-child{
	border-color: #F2473D;
}

.chargecash>ul>li:first-child>.trangle{
	border-bottom-color: #F2473D;
	border-right-color: #F2473D;
}

.chargecash>ol{
	width: 100%;
	height: 1.8rem;
	background: #fff;
}

.chargecash>ol>li{
	width: 100%;
	height: 0.44rem;
	line-height: 0.44rem;
	text-indent: 0.1rem;
	font-size: 0.14rem;
	position: relative;
	border-bottom: 1px solid #F0F0F0;
}

.chargecash>ol>li>img{
	width: 0.16rem;
    vertical-align: middle;
    margin-right: 0.1rem;
}

.chargecash>ol>li>.pay_circle{
	width: 0.18rem;
	height: 0.18rem;
	border-radius: 0.09rem;
	border: 1px solid #F0F0F0;
	position: absolute;
	right: 0.1rem;
	top: 0.13rem;
}

.chargecash>button{
	width: 2.9rem;
	height: 0.4rem;
	font-size: 0.14rem;
	background: #F2473D;
	text-align: center;
	line-height: 0.4rem;
	border-radius: 0.05rem;
	margin: auto;
	margin-top: 0.15rem;
	border: none;
	display: block;
}

.chargecash>button>a{
	color: #fff;
}
