.share{
	width: 100%;
	min-height: 100%;
	background: rgba(0,0,0,0.5);
	position: absolute;
	top: 0;
	left: 0;
	z-index: 22222222;
	display: none;
}

.sharemethod{
	width: 100%;
	height: auto;
	position: absolute;
	left: 0;
	bottom: 0;
}

.sharemethod>img{
	width: 100%;
