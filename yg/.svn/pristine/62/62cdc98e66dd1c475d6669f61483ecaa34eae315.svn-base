/*suggest*/
.suggest_form{
	width: 3rem;
	height: auto;
	font-size: 0.12rem;
	margin: auto;
	margin-top: 0.63rem;
}

.suggest_form>ul>li{
	width: 100%;
	height: 0.4rem;
	line-height: 0.4rem;
	border-bottom: 1px solid #F0F0F0;
	background: #fff;
}

.suggest_form>ul>li>span{
	margin-left: 0.15rem;
}

.suggest_form>ul>li>select{
	width: auto;
	float: right;
	height: 100%;
	border: none;
	display: block;
	appearance: none;
	color: #00a0e9;
	outline: none;
}
.suggest_form>ul>li:nth-child(4){
	height: 1.4rem;
	margin-top: 0.2rem;
}
.suggest_form>ul>li>textarea{
	height: 1.4rem;
	width: 100%;
	border: none;
	text-indent: 0.15rem;
}

.suggest_form>button{
	width: 3rem;
	height: 0.4rem;
	background: #f2473d;
	font-size: 0.15rem;
	text-align: center;
	line-height: 0.4rem;
	color: #fff;
	border: none;
	border-radius: 0.05rem;
	margin-top: 0.2rem;
}

/*sunok*/

.sunok{
	width: 100%;
