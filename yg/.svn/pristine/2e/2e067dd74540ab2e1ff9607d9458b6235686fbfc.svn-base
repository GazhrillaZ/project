header{
	font-size: 0.18rem;
	line-height: 0.43rem;
	color: #fff;
	text-align: center;
}

header>a{
	display: block;
    position: absolute;
    width: 0.14rem;
    height: 0.44rem;
    background: url(../img/return.png) no-repeat left center;
    background-size: 0.1rem;
    margin-left: 0.1rem;
}

section{
	margin-top: 0.43rem;
	width: 100%;
	border-top: 1px solid #f8f8f8;
	box-sizing: border-box;
}

section>a{
	width: 96.875%;
	height: 0.52rem;
	border-bottom: 1px solid #e2e2e2;
	float: right;
	font-size: 0.15rem;
	color: #333;
	line-height: 0.52rem;
}

section>a>img{
	width: 0.21rem;
	margin-left: 0.15rem;
	margin-right: 0.17rem;
	vertical-align: middle;
}
