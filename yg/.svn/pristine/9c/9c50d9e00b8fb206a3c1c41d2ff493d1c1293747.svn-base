.redpack_topmenu{
	width: 100%;
	height: 0.4rem;
	background: #fff;
	margin-top: 0.43rem;
}

.redpack_topmenu>li{
	width: 0.5rem;
	height: 0.4rem;
	line-height: 0.4rem;
	border-bottom: 0.02rem solid transparent;
	font-size: 0.15rem;
	color: #3e3a39;
	float: left;
	margin-left:0.283rem ;
	margin-right: 0.283rem;
	box-sizing: border-box;
	text-align: center;
}

.redpack_topmenu>.redpackBtn_active{
	color: #f2473d;
	border-color: #f2473d;
}

.redpack_detail{
	width: 3rem;
	height: 5rem;
	margin: auto;
	margin-top: 0.22rem;
}

.redpack_detail>ul{
	width: 100%;
	height: 100%;
	display: none;
}

.redpack_detail>.detail_active{
	display: block;
}

.redpack_detail>ul>li{
	width: 100%;
	height: 0.95rem;
	background: #e0e0e0;
	border-top: 1px solid #e0e0e0;
	border-left: 0.1rem solid #666666;
	box-sizing: border-box;
	margin-bottom: 0.2rem;
}

.some_text{
	width: 2rem;
	height: 0.6rem;
	margin-top: 0.2rem;
	margin-left: 0.1rem;
	float: left;
}

.some_text>p{
	font-size: 0.1rem;
	color:#666666;
	line-height: 0.18rem;
}

.some_text>p:first-child{
	font-size: 0.15rem;
	color:  #666666;
	line-height: 0.23rem;
	font-weight: bold;
}


.some_number{
	width: 0.7rem;
	height: 0.75rem;
	float: right;
	margin-top: 0.2rem;
	margin-right: 0.09rem;
}

.some_number>p{
	font-size: 0.1rem;
	color: #666666;
	text-align: right;
}


.some_number>p>span{
	font-size: 0.25rem;
	color: #666666;
}

.some_number>a{
	width: 0.55rem;
	height: 0.2rem;
	border-radius: 0.2rem;
	background: #666666;
	color: #fff;
	font-size: 0.1rem;
	line-height: 0.2rem;
	text-align: center;
	display: block;
	margin-left: 0.15rem;
}

.redpack_detail>ul:first-child>li{
	background: #fff;
	border-left:0.1rem solid #f2473d;
	border-top: 1px solid #fff;
}

.redpack_detail>ul:first-child>li>.some_text>p:first-child{
	color:#373737;
}

.redpack_detail>ul:first-child>li>.some_number>a{
	background: #f2473d;
	
}

.redpack_detail>ul:first-child>li>.some_number>p>span{
	color:  #373737;
}



